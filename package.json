{
  "name": "uniapp-easycom-types",
  "version": "1.0.2",
  "description": "Generate the interface for uniapp easycom",
  "type": "module",
  "keywords": [
    "uniapp",
    "easycom"
  ],
  "author": "Hayden <hayden.mike.zhang@gmail.com>",
  "packageManager": "pnpm@10.8.0",
  "repository": "hayden-fr/uniapp-easycom-types",
  "license": "MIT",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vitest dev",
    "build": "tsup src/*.ts --format cjs,esm --dts --splitting --clean",
    "test": "vitest --coverage"
  },
  "dependencies": {
    "fast-glob": "^3.3.3"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitest/coverage-v8": "^3.1.2",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vite": "^5.2.8",
    "vitest": "^3.1.2"
  },
  "peerDependencies": {
    "vite": "^5.2.8"
  }
}
